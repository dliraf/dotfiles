{
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.gitattributes": true,
        "**/.github": true,
        "**/.gitignore": true,
        "**/.gradle": true,
        "**/.project": true,
        "**/.settings": true,
        "**/bin": true,
        "**/build": true,
        "**/gradle": true
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m",
    "todohighlight.defaultStyle": {
        "color": "red",
        "backgroundColor": "rgba(0,0,0,.2)",
        "overviewRulerColor": "#ffab00",
        "cursor": "pointer",
        "border": "1px solid #eee",
        "borderRadius": "2px",
        "isWholeLine": false,
    },
    "todohighlight.keywords": [
        {
            "text": "$todo",
            "color": "red",
            "border": "1px solid red",
        },
        {
            "text": "TODO",
            "color": "red",
            "border": "1px solid red",
        }
    ],
    "todohighlight.include": [
        "**/*.java",
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss"
    ],
    "window.zoomLevel": 0,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Dracula",
    "java.semanticHighlighting.enabled": true,
    "cSpell.userWords": [
        "autoconfigure",
        "grpc",
        "grpcstarter",
        "springframework",
        "techyon"
    ],
    "go.useLanguageServer": true,
    "editor.fontSize": 12,
    "editor.fontLigatures": true,
    "indentRainbow.colors": [
        "rgba(20,20,20,0.3)",
        "rgba(50,50,50,0.3)",
        "rgba(80,80,80,0.3)",
        "rgba(110,110,110,0.3)",
        "rgba(140,140,140,0.3)"
    ],
    "indentRainbow.errorColor": "rgba(128,32,32,0)",
    "java.imports.gradle.wrapper.checksums": [
        {
            "sha256": "96f793a18e056c23ffeec67c1f3bb8eccff5a4a407fc9ceac183527e7eedf4b6",
            "allowed": true
        }
    ],
    "todo-tree.tree.showScanModeButton": false,
    "dashboard.projectData": null,
    "workbench.startupEditor": "newUntitledFile",
    "go.toolsGopath": "~/.go"
    
}
